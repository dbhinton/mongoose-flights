<%- include('../partials/header') %>
<ul>
  <% flightArr.forEach(function(f) { %>
        <h2><li>Flight Number: <%= f.flightNo %> </li>
        <li>Airline: <%= f.airline %> </li>
        <li>Airport: <%= f.airport %> </li>
        <li>Departure Date: <%= f.departs %> </li><br>
        <a class = "detail" href="/flights/<%= f._id %> ">DETAIL</a>
  <% }) %>
</ul>
    <%- include('../partials/footer') %>